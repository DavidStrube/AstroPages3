<div id="sketch"></div>

<script>
	import p5 from 'p5';
	const mySketch = (p) => {
		p.setup = () => {
			p.createCanvas(400,400);
		};

		p.draw = () => {
			p.background(220);
			p.fill('yellow');
			p.stroke(0,100,0);
			p.strokeWeight(5);
			p.ellipse(p.mouseX,p.mouseY,200);
		};
	};
	new p5(mySketch,document.getElementById('sketch'));
</script>